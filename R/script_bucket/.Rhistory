library(frequtils)
data(gas)
x <- gas$C4.alkylbenzene.unid.5
hist(x)
# Standardize the data
z.emp <- (x-mean(x))/sd(x)
# QQ-plot wrt/ standard normal distribution:
qqnorm(z.emp, xlim=c(-3,3), ylim=c(-3,3))
qqline(z.emp)
# A more general way to make a QQ-plot:
ref.dist.samp <- rnorm(length(x), mean = 0, sd = 1)
qqplot(ref.dist.samp, z.emp, main="QQ-Plot", xlim=c(-3,3), ylim=c(-3,3))
qqline(z.emp, distribution = function(p){qnorm(p, mean = 0, sd = 1)})
mu.log    <- 5.5
sigma.log <- 32
# Pr(-7 < log(X) < 6): (cf. log-amounts in cv_rsd.R)
plnorm(6, meanlog=mu.log, sdlog=sigma.log) - plnorm(-7, meanlog=mu.log, sdlog=sigma.log)
# How many ng is the range?:
exp(6)
exp(-7)
# Extra: What does the distribution with these parameters look like?
xx <- seq(from=0, to=10, length.out=1000)
yy <- dlnorm(xx, meanlog=mu.log, sdlog=sigma.log)
plot(xx,yy, typ="l")
samp <- rlnorm(10000, meanlog=mu.log, sdlog=sigma.log)
hist(log(samp))
# Pr( X > 3.418)
1 - pchisq(q = 3.418, df = 1)
# b. Pr( X < 1.83)
pchisq(q = 1.83, df = 1)
# c. Pr(0.74 < X < 2.02)
pchisq(q = 2.02, df = 1) - pchisq(q = 0.74, df = 1)
# a. Pr(24.5 < X < 26.3)
pnorm(26.3, mean = 27, sd = 3.1)
# a. Pr(24.5 < X < 26.3)
pnorm(26.3, mean = 27, sd = 3.1) - pnorm(24.5, mean = 27, sd = 3.1)
# a. Pr(25.4 < X < 26.3)
pnorm(26.3, mean = 27, sd = 3.1) - pnorm(25.4, mean = 27, sd = 3.1)
# Pr(c < Z) = 0.278 then c = ?:
qnorm(0.278, mean = 0, sd = 1)
# b. Standardization equation is: Z = (X - mu)/sigma
X     <- 4.6
mu    <- 3.5
sigma <- 1.2
Z <- (X - mu)/sigma
Z
# c. X = mu + Z*sigma
Z     <- 1.5
mu    <- 1.3
sigma <- 5.6
X <- mu + Z*sigma
X
# c. X = mu + Z*sigma
Z     <- 1.5
mu    <- 1.3
sigma <- 5.6
X <- mu + sigma*Z
X
X <- mu + sqrt(sigma.sq)*Z
sigma.sq <- 5.6
X <- mu + sqrt(sigma.sq)*Z
X
# d. Pr(27 < X < 32) mean = 30.5 and variance = 259.21
pnorm(32, mean = 30.5, sd = sqrt(259.21)) - pnorm(27, mean = 30.5, sd = sqrt(259.21))
# e. Pr(c < X) = 0.832 with mean 56.2 and standard deviation 37.8; then c = ?:
qnorm(0.832, mean = 56.2, sd = 37.8)
